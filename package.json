{
  "name": "repro-github-serverless",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "vercel": "^28.4.17"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.15.0"
}
